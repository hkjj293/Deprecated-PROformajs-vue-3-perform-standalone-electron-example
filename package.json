{
  "name": "proformajs-vue3-perform-standalone",
  "version": "0.0.0",
  "description": "Proformajs perform package standalone use case",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start-dev": "electron .",
    "start": "electron-forge start",
    "dev": "vite dev",
    "build": "npm run build --workspaces && vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "npm run lint --workspaces && eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "npm run format --workspaces && prettier --write src/",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@openclinical/proformajs": "^0.9.0",
    "bootstrap": "^5.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "cypress": "^12.7.0",
    "electron": "^24.2.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "vite": "^4.1.4"
  },
  "workspaces": [
    "packages/tools",
    "packages/map",
    "packages/perform"
